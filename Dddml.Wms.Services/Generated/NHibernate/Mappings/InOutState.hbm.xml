	<property name="DropShipBusinessPartnerId"  />

	<property name="CreatedBy"  />

	<property name="CreatedAt"  />

	<property name="UpdatedBy"  />

	<property name="UpdatedAt"  />

	<property name="Active"  />

	<property name="Deleted"  />


  </class>
  
</hibernate-mapping>

