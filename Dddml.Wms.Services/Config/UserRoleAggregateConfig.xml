<?xml version="1.0" encoding="UTF-8" ?>
<objects xmlns="http://www.springframework.net">


  <object id="userRoleStateRepository" type="Dddml.Wms.Domain.NHibernate.NHibernateUserRoleStateRepository, Dddml.Wms.Services">
    <property name="SessionFactory" ref="NHibernateSessionFactory"/>
  </object>

  <object id="userRoleEventStore" type="Dddml.Wms.Domain.NHibernate.NHibernateUserRoleEventStore, Dddml.Wms.Services">
    <property name="SessionFactory" ref="NHibernateSessionFactory"/>
