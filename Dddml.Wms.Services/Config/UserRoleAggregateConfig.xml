  <object id="userRoleApplicationService" type="Dddml.Wms.Domain.NHibernate.UserRoleApplicationService, Dddml.Wms.Services">
    <constructor-arg ref="userRoleEventStore"/>
    <constructor-arg ref="userRoleStateRepository"/>
  </object>

</objects>

